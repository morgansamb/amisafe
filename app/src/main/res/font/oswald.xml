<?xml version="1.0" encoding="utf-8"?>
<font-family
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/oswald_medium"

        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/oswald_medium"/>

    <font
        android:font="@font/oswald_semi_bold"
        android:fontStyle="normal"
        android:fontWeight="300"

        app:font="@font/oswald_semi_bold"
        app:fontStyle="normal"
        app:fontWeight="300" />

    <font
        android:font="@font/oswald_bold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/oswald_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <font
        android:font="@font/oswald_regular"
        android:fontStyle="normal"
        android:fontWeight="100"

        app:font="@font/oswald_regular"
        app:fontStyle="normal"
        app:fontWeight="100" />

</font-family>